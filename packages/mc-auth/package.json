{
  "name": "@milescreative/mc-auth",
  "version": "0.0.7",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.11.24",
    "msw": "^2.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.9.2",
    "cookie": "^0.6.0",
    "hono": "^4.6.14",
    "kysely": "^0.27.2",
    "zod": "^3.22.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "exports": {
    ".": {
      "development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/*"
      ]
    }
  }
}
